# reference : https://cheese10yun.github.io/intellij-boot-run-config/
server:
  port: 19372
      
spring:
  application:
    name: micro-iot-service
  main:
    allow-bean-definition-overriding: true

eureka:
  client:
    serviceUrl:
      defaultZone: http://127.0.0.1:19771/eureka
  instance:
    instanceId: group1-consumer2
    
application:
  group: group1
  instance: consumer2

instance:
  topologyMap:
    backupCount: 3